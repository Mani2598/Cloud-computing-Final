      "source": [
        "sc=SparkContext.getOrCreate()\n",
        "\n",
        "fileRDD=sc.textFile(\"maninder.banala.txt\")\n",
        "result=fileRDD.flatMap(lambda x: x.split(\" \")).map(lambda word:(word,1)).reduceByKey(lambda a,b: a+b)\n",
        "\n",
        "result.coalesce(1).saveAsTextFile(\"result.txt\")\n",
        "\n",
        "res=result.collect()\n",
        "print(res)"
      ],
